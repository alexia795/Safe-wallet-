{
  "version": 2,
  "name": "safe-wallet-pro",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_NAME": "Safe Wallet Pro",
      "NEXT_PUBLIC_NETWORK": "mainnet",
      "NEXT_PUBLIC_RPC_URL": "https://eth-mainnet.g.alchemy.com/v2/XuPZE3fUgxJ2AwDHYiSLzBVscOVcg9dy",
      "NEXT_PUBLIC_SAFE_OWNER": "0xFDf84a0e7D07bC56f7De56696fc409704cC83a24"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next"
}
